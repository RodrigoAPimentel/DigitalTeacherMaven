<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<HBox alignment="CENTER" spacing="20.0" styleClass="background" stylesheets="@../../CSS/HBoxDrawer.css" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.pimentel.digitalteacher.controller.HBoxDrawerFuncionarioController">
	<children>
		<VBox fx:id="vboxAddUsuario" alignment="CENTER" onMouseClicked="#clickedVboxAddUsuario" styleClass="botao-controle" stylesheets="@../../CSS/HBoxDrawer.css">
			<children>
				<ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
					<image>
						<Image url="@../../IMAGENS/ICONES/usuarioADD.png" />
					</image>
				</ImageView>
				<Label text="Adicionar">
					<font>
						<Font name="Times New Roman Bold Italic" size="8.0" />
					</font>
				</Label>
			</children>
		</VBox>
		<VBox fx:id="vboxRemoveUsuario" alignment="CENTER" layoutX="10.0" layoutY="10.0" onMouseClicked="#clickedVboxRemoveUsuario" styleClass="botao-controle" stylesheets="@../../CSS/HBoxDrawer.css">
			<children>
				<ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
					<image>
						<Image url="@../../IMAGENS/ICONES/usuarioREMOVE.png" />
					</image>
				</ImageView>
				<Label text="Remover">
					<font>
						<Font name="Times New Roman Bold Italic" size="8.0" />
					</font>
				</Label>
			</children>
		</VBox>
		<VBox fx:id="vboxPesquisaUsuario" alignment="CENTER" layoutX="80.0" layoutY="10.0" onMouseClicked="#clickedVboxPesquisaUsuario" styleClass="botao-controle" stylesheets="@../../CSS/HBoxDrawer.css">
			<children>
				<ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
					<image>
						<Image url="@../../IMAGENS/ICONES/usuarioPESQUISA.png" />
					</image>
				</ImageView>
				<Label text="Pesquisa">
					<font>
						<Font name="Times New Roman Bold Italic" size="8.0" />
					</font>
				</Label>
			</children>
		</VBox>
	</children>
</HBox>
